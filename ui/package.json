    "babel-jest": "^26.5.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "dayjs": "^1.8.36",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "formik": "^2.1.4",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.3",
    "notistack": "^1.0.0",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-intl": "^5.8.1",
