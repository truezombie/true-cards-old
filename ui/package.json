{
  "name": "front-end",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --mode=development",
    "build:langs": "node scripts/mergeMessages.js",
    "build": "npm run build:langs && cross-env NODE_ENV=production webpack --mode=production",
    "test": "cross-env NODE_ENV=test jest --notify --detectOpenHandles --config=jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --watch --config=jest.config.js",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --colors --notify --detectOpenHandles --config=jest.config.js",
    "lint": "eslint --ext=.js,.ts,.tsx src",
    "lint:fix": "eslint --ext=.js,.ts,.tsx src --fix",
    "check:ts": "tsc --noEmit",
    "check:lint": "npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check:lint",
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "author": "Oleh Ovcharenko",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.6",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/yup": "^0.29.7",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "4.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-watch": "^7.0.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.3.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "typescript": "^4.0.2",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@apollo/client": "^3.2.2",
    "@apollo/react-testing": "^4.0.0",
